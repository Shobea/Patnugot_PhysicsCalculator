<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.patnugot_physicscalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Patnugot_PhysicsCalculator">
        <activity
            android:name=".View.VolumeAreaAnswers"
            android:exported="false" />
        <activity
            android:name=".View.PhysicsAnswers"
            android:exported="false" />
        <activity
            android:name=".View.VolumeArea"
            android:exported="false" />
        <activity
            android:name=".View.Physics"
            android:exported="false" />
        <activity
            android:name=".View.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>